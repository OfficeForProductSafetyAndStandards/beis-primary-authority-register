        </svg>
      </a>
    </div>
    <div class="govuk-header__content">
      <a href="{{ url('<front>') }}" class="govuk-header__link govuk-header__service-name">
        {{ site_name }}
      </a>
      {{ page.navigation }}
    </div>
  </div>
</header>

<div class="govuk-width-container">
  {% if govuk_phase_banner %}
    {{  govuk_phase_banner }}
  {% endif %}

  {% if page.pre_content %}
    {{ page.pre_content }}
  {% endif %}

  {% if page.breadcrumb and show_breadcrumb %}
    {{ page.breadcrumb }}
  {% endif %}

  {% if page.highlighted %}
    {{ page.highlighted }}
  {% endif %}

  {%
    set main_classes = [
    'govuk-main-wrapper',
    'govuk-body-m',
    show_breadcrumb ? 'has-breadcrumb',
    page.content.govuk_local_tasks|render|striptags|trim is not empty ? 'has-local-tasks'
  ]
  %}
  <main {{ attributes.addClass(main_classes) }} id="main-content" role="main">
    <div class="govuk-grid-row">
      {% if page.sidebar_first %}
        <div class="govuk-grid-column-two-thirds">
      {% else %}
        <div class="govuk-grid-column-full">
      {% endif %}
        {{ page.content }}
      </div>
      {% if page.sidebar_first %}
        <div class="govuk-grid-column-one-third">
          {{ page.sidebar_first }}
        </div>
      {% endif %}
    </div>
  </main>
</div>
<footer class="govuk-footer " role="contentinfo">
    <div class="govuk-width-container ">
      <div class="govuk-footer__meta">
        <div class="govuk-footer__meta-item govuk-footer__meta-item--grow">
          {% if page.footer %}
            {{ page.footer }}
          {% endif %}
          {{ govuk_footer }}
        </div>
        {{ govuk_meta }}
      </div>
    </div>
  </footer>
<!-- ends page template -->
