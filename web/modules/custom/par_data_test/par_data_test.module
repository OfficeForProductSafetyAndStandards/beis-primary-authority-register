<?php

/**
 * @file
 * Contains par_data_test.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function par_data_test_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the par_data_test module.
    case 'help.page.par_data_test':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Test data for the PAR Data model') . '</p>';
      return $output;

    default:
  }
}

function par_data_test_create_entities() {
  $authority = \Drupal\par_data\Entity\ParDataAuthority::create([
    'type' => 'authority',
    'title' => 'Lower East Side Borough Council',
    'uid' => 1,
    'name' => 'Lower East Side Borough Council',
    'authority_type' => 'Local Authority',
    'nation' => 'USA',
    'ons_code' => '123456',
  ]);
  $alternative_authority = \Drupal\par_data\Entity\ParDataAuthority::create([
    'type' => 'authority',
    'title' => 'Upper West Side Borough Council',
    'uid' => 1,
    'name' => 'Upper West Side Borough Council',
    'authority_type' => 'Local Authority',
    'nation' => 'USA',
    'ons_code' => '123456',
  ]);
}
