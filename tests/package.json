{
  "name": "cucumber-boilerplate",
  "version": "1.1.1",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "scripts": {
    "test": "npm run test:validate && npm run test:features",
    "test:features": "./node_modules/.bin/wdio wdio.DEV.conf.js",
    "test:validate": "eslint 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/cucumber-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/cucumber-boilerplate/issues"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "test",
    "selenium"
  ],
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "~6.24.0",
    "babel-register": "~6.24.0",
    "chai": "~4.0.0",
    "cucumber": "~2.2.0",
    "wdio-cucumber-framework": "~0.3.0",
    "wdio-phantomjs-service": "~0.2.2",
    "wdio-selenium-standalone-service": "~0.0.8",
    "wdio-spec-reporter": "*",
    "webdriverio": "~4.8.0"
  },
  "devDependencies": {
    "babel-jest": "~20.0.0",
    "babel-polyfill": "~6.23.0",
    "eslint": "~3.19.0",
    "eslint-config-airbnb-base": "~11.2.0",
    "eslint-plugin-import": "~2.3.0",
    "http-server": "~0.10.0",
    "jest": "~20.0.0"
  }
}
