{
  "name": "cucumber-boilerplate",
  "version": "1.1.1",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "scripts": {
    "test": "npm run test:build",
    "test:build": "./node_modules/.bin/wdio wdio.BUILD.conf.js",
    "test:smoke": "./node_modules/.bin/wdio wdio.SMOKE.conf.js",
    "test:dev": "./node_modules/.bin/wdio wdio.DEV.conf.js",
    "test:validate": "eslint 'src/**/*.js'",
    "generatehtmlreport": "marge reports/report.json --reportTitle 'Test Results'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/cucumber-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/cucumber-boilerplate/issues"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "test",
    "selenium"
  ],
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "^21.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cucumber": "^3.2.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "forever": "^0.15.3",
    "fs-extra": "^5.0.0",
    "http-server": "^0.10.0",
    "jest": "^21.2.1",
    "wdio-browserstack-service": "^0.1.14",
    "wdio-chromedriver-service": "^0.1.2",
    "wdio-cucumber-framework": "^1.1.0",
    "wdio-json-reporter": "^0.2.1",
    "wdio-mochawesome-reporter": "^1.3.0",
    "wdio-screenshot": "^0.6.0",
    "wdio-selenium-standalone-service": "^0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "webdrivercss": "^1.1.10",
    "webdriverio": "^4.12.0"
  }
}
