{
    "name": "beis/primary-authority-register",
    "description": "The beta platform for BEIS PAR.",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/do-file-cache-psr-6"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-audit-log"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-trance"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-password-strength"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-invite"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/companieshouse-api-php-client"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8.3.0",
        "bjeavons/zxcvbn-php": "~1.3.0",
        "commerceguys/intl": "^1.0",
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/audit_log": "3.0.x-dev",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/block_classes": "^1.0",
        "drupal/cacheexclude": "^2.0",
        "drupal/chosen": "~3.0.0",
        "drupal/config_filter": "^2.7",
        "drupal/config_readonly": "^1.0",
        "drupal/config_split": "~2.0.0",
        "drupal/config_update": "^2.0@alpha",
        "drupal/core-composer-scaffold": "~10.2.0",
        "drupal/core-recommended": "10.3.11",
        "drupal/csv_serialization": "^4.0",
        "drupal/ctools": "^4.1",
        "drupal/default_content": "^2.0@alpha",
        "drupal/email_registration": "^2.0@RC",
        "drupal/features": "~3.14.0",
        "drupal/flysystem": "^2.2@alpha",
        "drupal/flysystem_s3": "^2.1",
        "drupal/google_tag": "^2.0",
        "drupal/govuk_notify": "^3.1",
        "drupal/govuk_theme": "^2.1",
        "drupal/health_check": "^3.1",
        "drupal/http_response_headers": "~2.0.0",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/invite": "8.x-1.x-dev",
        "drupal/jquery_ui": "^1.7",
        "drupal/jquery_ui_button": "^2.1",
        "drupal/jquery_ui_checkboxradio": "^2.1",
        "drupal/jquery_ui_controlgroup": "^2.1",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_tabs": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/js_cookie": "^1.0",
        "drupal/login_destination": "^2.0@beta",
        "drupal/maillog": "~1.1.0",
        "drupal/mailsystem": "^4.5",
        "drupal/masquerade": "~2.0.0",
        "drupal/menu_link_attributes": "^1.5",
        "drupal/message": "1.4",
        "drupal/message_expire": "~1.0.0",
        "drupal/message_notify": "^1.4",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6",
        "drupal/pagerer": "^3.1",
        "drupal/password_policy": "^4.0",
        "drupal/queue_ui": "~3.1.0",
        "drupal/raven": "^5.0",
        "drupal/redis": "^1.8",
        "drupal/registered_organisations": "1.0.x-dev",
        "drupal/robotstxt": "^1.6",
        "drupal/role_delegation": "~1.2.0",
        "drupal/search_api": "^1.35",
        "drupal/search_api_opensearch": "^2.1",
        "drupal/seckit": "^2.0",
        "drupal/symfony_mailer": "~1.4.0",
        "drupal/token": "^1.15",
        "drupal/tracer": "~1.0.0",
        "drupal/trance": "8.x-1.x-dev",
        "drupal/twig_field_value": "~2.0.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^4.3",
        "drupal/vbo_export": "^4.1",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_contextual_filters_or": "~1.4.0",
        "drupal/views_data_export": "^1.5",
        "drupal/xmlsitemap": "~1.5.0",
        "drush/drush": "^12.5",
        "predis/predis": "~2.2.2",
        "rapidwebltd/php-uk-bank-holidays": "~5.0.0",
        "vlucas/phpdotenv": "~5.6.0"
    },
    "require-dev": {
        "behat/mink-browserkit-driver": "^2.2.0",
        "brianium/paratest": "^6.0.0",
        "drupal/config_inspector": "^2.1.3",
        "drupal/core-dev": "~10.2.0",
        "drupal/devel": "^5.2",
        "drupal/webprofiler": "10.1.4",
        "mglaman/drupal-check": "^1.4",
        "palantirnet/drupal-rector": "^0.19.0",
        "phpspec/prophecy": "^1.8.0",
        "phpspec/prophecy-phpunit": "^2",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./web"
            },
            "symlink": true,
            "overwrite": true,
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/web.config": false,
                "[web-root]/.htaccess": {
                    "mode": "append",
                    "append": "patches/well-known-traffic.txt"
                }
            }
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "Add events for matching entity hooks": "./patches/event-entity-hook.patch"
            },
            "drupal/govuk_notify": {
                "Message body formatting array to string": "https://www.drupal.org/files/issues/2021-08-11/3227617-2-govuk-notify-message-body-formatting.diff"
            },
            "drupal/pagerer": {
                "Pagerer upgrade issue": "https://git.drupalcode.org/project/pagerer/-/merge_requests/18.diff"
            },
            "twistor/flysystem-stream-wrapper": {
                "Fix PHP error on flystem (see https://github.com/twistor/flysystem-stream-wrapper/issues/27 & https://www.drupal.org/project/flysystem/issues/3387094": "patches/flysystem-deprecated-dynamic-prop.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "alias drush='/var/www/par/vendor/drush/drush/drush'"
        ]
    }
}
