{
    "name": "beis/primary-authority-register",
    "description": "The beta platform for BEIS PAR.",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/UKGovernmentBEIS/csv-validator"
        },
        {
            "type": "vcs",
            "url": "https://github.com/UKGovernmentBEIS/drupal-trance"
        },
        {
            "type": "vcs",
            "url": "https://github.com/UKGovernmentBEIS/password-strength"
        },
        {
            "type": "vcs",
            "url": "https://github.com/UKGovernmentBEIS/companieshouse-api-php-client"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "~8.1.0",
        "aws/aws-sdk-php": "~3.209.0",
        "bjeavons/zxcvbn-php": "~1.3.0",
        "commerceguys/intl": "^1.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "deblan/csv-validator": "dev-master",
        "drupal/a11y_autocomplete_element": "^1.0",
        "drupal/address": "~1.11.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/audit_log": "~3.0.0",
        "drupal/autologout": "~1.3.0",
        "drupal/better_exposed_filters": "~5.0.0",
        "drupal/big_pipe_sessionless": "~2.0.0",
        "drupal/block_classes": "~1.0.0",
        "drupal/chosen": "~3.0.0",
        "drupal/clamav": "~2.0.0",
        "drupal/config_filter": "~1.8.0",
        "drupal/config_readonly": "~1.0.0",
        "drupal/config_split": "~1.7.0",
        "drupal/config_update": "~1.7.0",
        "drupal/core": "~9.5.0",
        "drupal/core-composer-scaffold": "~9.3",
        "drupal/core-recommended": "~9.3",
        "drupal/csv_serialization": "~2.1",
        "drupal/default_content": "~2.0.0",
        "drupal/email_registration": "~1.1.0",
        "drupal/features": "~3.12.0",
        "drupal/flysystem": "~2.0.0",
        "drupal/flysystem_s3": "~2.0.0",
        "drupal/google_analytics": "~4.0.0",
        "drupal/govuk_cookies": "1.0.x-dev",
        "drupal/govuk_notify": "~2.1.0",
        "drupal/health_check": "1.3.0",
        "drupal/http_response_headers": "~2.0.0",
        "drupal/inline_entity_form": "~1.0.0",
        "drupal/invite": "~1.0.0",
        "drupal/jquery_ui": "~1.4.0",
        "drupal/login_destination": "~2.0.0",
        "drupal/maillog": "~1.0.0",
        "drupal/mailsystem": "~4.3.0",
        "drupal/masquerade": "~2.0.0",
        "drupal/menu_link_attributes": "~1.1.0",
        "drupal/message": "~1.2.0",
        "drupal/message_expire": "~1.0.0",
        "drupal/message_notify": "~1.2.0",
        "drupal/migrate_file_to_media": "2.0.9",
        "drupal/no_autocomplete": "~1.6.0",
        "drupal/pagerer": "~2.1.0",
        "drupal/password_policy": "~3.0.0",
        "drupal/password_strength": "dev-8.x-2.x",
        "drupal/queue_ui": "~2.2.0",
        "drupal/range": "~1.3.0",
        "drupal/raven": "~3.2.0",
        "drupal/redis": "~1.5.0",
        "drupal/registered_organisations": "1.0.x-dev",
        "drupal/robotstxt": "~1.4.0",
        "drupal/role_delegation": "~1.1.0",
        "drupal/search_api": "~1.23.0",
        "drupal/search_api_opensearch": "~1.0.0",
        "drupal/symfony_mailer": "~1.0.0",
        "drupal/token": "~1.9.0",
        "drupal/trance": "8.x-1.x-dev",
        "drupal/twig_field_value": "~2.0.0",
        "drupal/twig_tweak": "~3.1.0",
        "drupal/ultimate_cron": "~2.0.0",
        "drupal/upgrade_status": "^3.14.0",
        "drupal/vbo_export": "~3.4.0",
        "drupal/views_bulk_operations": "~4.0.0",
        "drupal/views_contextual_filters_or": "~1.2.0",
        "drupal/views_data_export": "~1.0.0",
        "drupal/views_field_view": "~1.0.0",
        "drupal/xmlsitemap": "~1.0.0",
        "drush/config-extra": "~1.0.0",
        "drush/drush": "~10.4",
        "league/flysystem": "1.0.69",
        "league/flysystem-aws-s3-v3": "1.0.25",
        "predis/predis": "~1.1.1",
        "rapidwebltd/php-uk-bank-holidays": "~5.0.0",
        "vlucas/phpdotenv": "~2.4.0"
    },
    "require-dev": {
        "behat/mink": "^1.8",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.4",
        "brianium/paratest": "^6.0",
        "drupal/coder": "^8.3.10",
        "drupal/config_inspector": "^1.0",
        "drupal/console": "^1.0",
        "drupal/core-dev": "^9.1",
        "drupal/devel": "~4.1.0",
        "jcalderonzumba/gastonjs": "^1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
        "mglaman/drupal-check": "^1.0",
        "mikey179/vfsstream": "^1.2",
        "phpspec/prophecy": "^1.8",
        "phpspec/prophecy-phpunit": "^2",
        "phpunit/phpunit": "^9.0",
        "symfony/css-selector": "^4.0.0",
        "symfony/debug": "^4.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./web"
            },
            "symlink": true,
            "overwrite": true,
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/web.config": false
            }
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/clamav": {
                "Add support for ClamAV via a REST daemon": "./patches/clamav_add_rest_client.patch"
            },
            "drupal/core": {
                "Ensure the table alias is no longer than 60 characters long": "https://www.drupal.org/files/issues/2022-06-30/571548-165.patch",
                "Form API #ajax not working on link element": "https://www.drupal.org/files/issues/2023-01-11/core-allow-form-ajax-for-links-2915954-9.5.1.patch",
                "Add events for matching entity hooks": "https://www.drupal.org/files/issues/2022-12-22/2551893-99.patch",
                "Non-exposed and exposed default sort order": "https://www.drupal.org/files/issues/2021-02-16/3120847-20.patch",
                "PostgreSQL: email case insensitivity": "https://www.drupal.org/files/issues/2022-12-19/drupal-case_sensitive_email-2490294-58-d95.patch",
                "Add aria labelling for form states": "./patches/states-aria.patch",
                "Extends IFE to prefix page title and use error messages in the summary": "./patches/inline_form_errors.patch"
            },
            "drupal/devel": {
                "Webprofiler related crash: requested a non-existent service": "https://gitlab.com/drupalspoons/devel/uploads/8c8ddd9716570d17fa46b9182d22aa7f/419-Webprofiler-related-crash-in-core-9.4.2.patch"
            },
            "drupal/flysystem": {
                "Add Drush commands for sending and receiving files": "https://www.drupal.org/files/issues/2021-01-14/3192777-2-Added_drush_commands.patch",
                "CSS and JS served through the flysystem.serve route can't be downloaded in maintenance mode": "https://www.drupal.org/files/issues/2019-06-13/serve_maintenance_page_assets-3061561-3.patch"
            },
            "drupal/govuk_notify": {
                "Message body formatting array to string": "https://www.drupal.org/files/issues/2021-08-11/3227617-2-govuk-notify-message-body-formatting.diff"
            },
            "drupal/redis": {
                "Unable to override default scheme with settings": "https://www.drupal.org/files/issues/2021-02-16/redis-support_tls_on_predis-3004561-33.patch",
                "Take into account CONFIG command may not work": "https://www.drupal.org/files/issues/2021-02-16/redis-allow_disabled_config-3173988-4.patch"
            },
            "drupal/role_delegation": {
                "Temporary patch to enable module uninstall": "./patches/tmp-patch-fix-for-d9-upgrade.patch"
            },
            "drupal/vbo_export": {
                "Allow output file scheme to be chosen": "https://www.drupal.org/files/issues/2021-11-23/allow_configurable_scheme-2988257-18.patch"
            },
            "drupal/views_data_export": {
                "Allow output file scheme to be chosen": "https://www.drupal.org/files/issues/2021-03-01/allow-configurable-filesystems_3200974-6.patch"
            },
            "drupal/a11y_autocomplete_element": {
                "Target GOVUK theme classes": "./patches/govuk-theme-classes.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "alias drush='/var/www/par/vendor/drush/drush/drush'"
        ]
    }
}
