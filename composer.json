{
    "name": "TransformCore/beis-par-beta",
    "description": "The beta platform for BEIS PAR.",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitnet.fr/kalpaitch/csv-validator.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "commerceguys/intl": "^1.0",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "deblan/csv-validator": "dev-master",
        "drupal-composer/drupal-scaffold": "~2.3.0",
        "drupal/address": "~1.4.0",
        "drupal/better_exposed_filters": "~3.0.0",
        "drupal/clamav": "~1.0.0",
        "drupal/config_readonly": "~1.0.0",
        "drupal/config_split": "~1.4.0",
        "drupal/config_update": "~1.3.0",
        "drupal/core": "~8.6.0",
        "drupal/csv_serialization": "~1.0.0",
        "drupal/default_content": "~1.0.0",
        "drupal/email_registration": "~1.0.0",
        "drupal/features": "~3.5.0",
        "drupal/file_entity": "~2.0.0",
        "drupal/flysystem": "~1.0.0",
        "drupal/flysystem_s3": "~1.0.0",
        "drupal/google_analytics": "~2.1.0",
        "drupal/govuk_notify": "~1.1.0",
        "drupal/health_check": "1.0.0",
        "drupal/http_response_headers": "2.x-dev#de12feba1ca817352cafae68e4731344a56341c5",
        "drupal/inline_entity_form": "~1.0.0",
        "drupal/invite": "1.x-dev#49677d79429f7a5c9b15f42e2b3c4625708559a8",
        "drupal/login_destination": "1.x-dev#54be8b89fdc073ca40af6b9b2eeb050e0aeb7908",
        "drupal/maillog": "1.x-dev",
        "drupal/mailsystem": "~4.1.0",
        "drupal/message": "~1.0.0",
        "drupal/message_notify": "~1.0.0@beta",
        "drupal/no_autocomplete": "~1.0.0",
        "drupal/pagerer": "~1.1.0",
        "drupal/password_policy": "3.x-dev#ef81a55ad49d7d4e18f493fad2284923d28bfe70",
        "drupal/password_strength": "~2.0.0",
        "drupal/queue_ui": "~2.0.0",
        "drupal/range": "~1.1.0",
        "drupal/raven": "~2.4.0",
        "drupal/robotstxt": "~1.1.0",
        "drupal/role_delegation": "~1.0.0",
        "drupal/swiftmailer": "~1.0.0-beta1",
        "drupal/trance": "~1.1.0",
        "drupal/twig_field_value": "~1.1.0",
        "drupal/twig_tweak": "~1.8.0",
        "drupal/ultimate_cron": "~2.0.0",
        "drupal/vbo_export": "~2.0.0",
        "drupal/views_bulk_operations": "~1.0.0",
        "drupal/views_contextual_filters_or": "~1.0.0",
        "drupal/views_data_export": "~1.0.0",
        "drupal/views_field_view": "~1.0.0",
        "drupal/xmlsitemap": "~1.0.0",
        "drush/config-extra": "~1.0.0",
        "drush/drush": "~8.1.0",
        "rapidwebltd/php-uk-bank-holidays": "~2.0.0",
        "vlucas/phpdotenv": "~2.4.0"
    },
    "require-dev": {
        "behat/mink": "1.7.x-dev",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "1.3.x-dev",
        "brianium/paratest": "^1.1",
        "drupal/config_inspector": "^1.0@beta",
        "drupal/console": "~1.8.0",
        "drupal/devel": "1.x-dev#7eb4b0c10f5f3b1bddf20d1ac907b04e1afde17d",
		"drupal/examples": "dev-1.x",
        "drupal/coder": "^8.2.12",
        "jcalderonzumba/gastonjs": "^1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
        "mikey179/vfsStream": "^1.2",
        "phpunit/phpunit": "^6.5",
        "phpspec/prophecy": "^1.4",
        "symfony/css-selector": "^3.4.0",
        "symfony/phpunit-bridge": "^3.4.3",
        "symfony/debug": "^3.4.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "excludes": [
                "robots.txt",
                "web.config"
            ]
        },
        "patches": {
            "drupal/clamav": {
                "Add support for ClamAV via a REST daemon": "./patches/clamav_add_rest_client.patch"
            },
            "drupal/core": {
                "file_exists() doesn't work with remote stream wrappers in file_cron()": "https://www.drupal.org/files/issues/file_exists_remote_streams_cron-2928889-2.patch",
                "PostgreSQL: deal with case insensitivity": "./patches/postgres_case_insensitivity-2464481.patch",
                "User email should not be case sensitive": "./patches/case_insensitive_login-2490294-31.patch",
                "Ensure the table alias is no longer than 60 characters long": "https://www.drupal.org/files/issues/drupal-571548-107-d8_0.patch",
                "Form API #ajax not working on link element": "https://www.drupal.org/files/issues/2018-03-12/core-allow-form-ajax-for-links-2915954-9.patch",
                "Add events for matching entity hooks": "https://www.drupal.org/files/issues/2551893-52.patch"
            },
            "drupal/default_content": {
                "Do not reimport existing entities": "https://www.drupal.org/files/issues/default_content-dont-reimport-existing-entities-2698425-99.patch",
                "Allow manual imports": "https://www.drupal.org/files/issues/allow_manual_imports-2640734-67.patch"
            },
            "drupal/file_entity": {
                "Provides support for s3 response objects": "./patches/file_entity_s3_file_support.patch"
            },
            "drupal/flysystem": {
                "Add Drush commands for sending and receiving files": "./patches/flysystem-drush-command.patch"
            },
            "drupal/flysystem_s3": {
                "Allow downloading of private files from remote S3 wrappers": "./patches/private_s3_file_download.patch"
            },
            "drupal/govuk_notify": {
                "Mail body formatting": "./patches/mail_body-govuk_notify.patch"
            },
            "drupal/health_check": {
                "Health Check doesn't check Drupal application": "https://www.drupal.org/files/issues/health_check_doesn_t-2892294-2.patch"
            },
            "drupal/invite": {
                "Warning: current() expects parameter 1 to be array": "https://www.drupal.org/files/issues/2018-06-01/warning-2976929.2.patch",
		        "Plugin not plugin": "https://www.drupal.org/files/issues/2018-06-07/plugin_not_plugin-2976921-2.patch"
            },
            "drupal/login_destination": {
                "One time login link": "./patches/one_time_login_redirection.patch",
                "SetOptions on null causing error": "https://www.drupal.org/files/issues/2018-03-09/toolbar_alter_error-2947616-11.patch"
            },
            "drupal/message": {
                "Return save status": "https://www.drupal.org/files/issues/2018-10-24/return_save_status-3008879-2.patch"
            },
            "drupal/password_policy": {
                "Remove setting of field_last_password_reset on install": "https://www.drupal.org/files/issues/2018-07-03/Do-not-set-user-fields-on-install_2983448_4.patch"
            },
            "drupal/password_strength": {
                "Change user notifications to explain reason for password rejection more clearly": "./patches/password_strength_dictionary.patch"
            },
            "drupal/vbo_export": {
                "Allow output file scheme to be chosen": "https://www.drupal.org/files/issues/2018-07-26/allow_configurable_scheme-2988257-5.patch"
            },
            "drush/drush": {
                "Fix SQL dump problem where extra params are passed when trying only to list tables": "./patches/3321-sql-dump-no-extra-fix.patch",
                "Refs #2079. Do not pass `$this->query_extra` to `pg_dump`": "./patches/drush-sqldump-postgres95.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer drupal-scaffold",
            "alias drush='/var/www/par-beta/vendor/bin/drush'"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    }
}
