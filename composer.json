{
    "name": "beis/primary-authority-register",
    "description": "The beta platform for BEIS PAR.",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/do-file-cache-psr-6"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-audit-log"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-trance"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-password-strength"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-flysystem-s3"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/drupal-invite"
        },
        {
            "type": "vcs",
            "url": "https://github.com/OfficeForProductSafetyAndStandards/companieshouse-api-php-client"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "bjeavons/zxcvbn-php": "~1.3.0",
        "commerceguys/intl": "^1.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "~1.11.0",
        "drupal/audit_log": "3.0.x-dev",
        "drupal/autologout": "~1.4.0",
        "drupal/better_exposed_filters": "~6.0.0",
        "drupal/block_classes": "~1.0.0",
        "drupal/chosen": "~3.0.0",
        "drupal/clamav": "~2.0.0",
        "drupal/config_filter": "~1.10.0",
        "drupal/config_readonly": "~1.0.0",
        "drupal/config_split": "~1.9.0",
        "drupal/config_update": "~2.0.0",
        "drupal/core-composer-scaffold": "~10.1.0",
        "drupal/core-recommended": "~10.1.0",
        "drupal/csv_serialization": "~3.0.0",
        "drupal/ctools": "~4.0.0",
        "drupal/default_content": "~2.0.0",
        "drupal/email_registration": "~1.2.0",
        "drupal/features": "~5.0.0",
        "drupal/flysystem": "~2.1.0",
        "drupal/flysystem_s3": "2.0.x-dev",
        "drupal/google_analytics": "~4.0.0",
        "drupal/govuk_cookies": "1.0.x-dev",
        "drupal/govuk_notify": "~2.2.0 || ~3.0.0",
        "drupal/govuk_theme": "~1.14.0",
        "drupal/health_check": "3.0.0",
        "drupal/http_response_headers": "~2.0.0",
        "drupal/inline_entity_form": "~1.0.0",
        "drupal/invite": "8.x-1.x-dev",
        "drupal/jquery_ui": "~1.6.0",
        "drupal/jquery_ui_tabs": "~2.0.0",
        "drupal/login_destination": "~2.0.0",
        "drupal/maillog": "~1.0.0",
        "drupal/mailsystem": "~4.4.0",
        "drupal/masquerade": "~2.0.0",
        "drupal/menu_link_attributes": "~1.3.0",
        "drupal/message": "~1.4.0",
        "drupal/message_expire": "~1.0.0",
        "drupal/message_notify": "~1.3.0",
        "drupal/migrate_tools": "^6",
        "drupal/migrate_plus": "^6",
        "drupal/pagerer": "~3.0.0",
        "drupal/password_policy": "~4.0.0",
        "drupal/queue_ui": "~3.1.0",
        "drupal/range": "~1.5.0",
        "drupal/raven": "~3.2.0",
        "drupal/redis": "~1.7.0",
        "drupal/registered_organisations": "1.0.x-dev",
        "drupal/robotstxt": "~1.5.0",
        "drupal/role_delegation": "~1.2.0",
        "drupal/search_api": "~1.29.0",
        "drupal/search_api_opensearch": "~2.0.0",
        "drupal/symfony_mailer": "~1.3.0",
        "drupal/token": "~1.12.0",
        "drupal/tracer": "~1.0.0",
        "drupal/trance": "8.x-1.x-dev",
        "drupal/twig_field_value": "~2.0.0",
        "drupal/twig_tweak": "~3.2.0",
        "drupal/ultimate_cron": "~2.0.0",
        "drupal/upgrade_status": "^4.0.0",
        "drupal/vbo_export": "~4.1.0",
        "drupal/views_bulk_operations": "~4.2.0",
        "drupal/views_contextual_filters_or": "~1.4.0",
        "drupal/views_data_export": "~1.3.0",
        "drupal/views_field_view": "~1.0.0",
        "drupal/xmlsitemap": "~1.5.0",
        "drush/drush": "~12.2",
        "league/flysystem": "1.0.69",
        "league/flysystem-aws-s3-v3": "1.0.25",
        "phpoffice/phpspreadsheet": "^1.6",
        "predis/predis": "~1.1.1",
        "rapidwebltd/php-uk-bank-holidays": "~5.0.0",
        "vlucas/phpdotenv": "~5.5.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.0",
        "drupal/config_inspector": "^2.1.3",
        "drupal/core-dev": "^10.1",
        "drupal/devel": "~5.1.0",
        "mglaman/drupal-check": "^1.4",
        "palantirnet/drupal-rector": "^0.15.0",
        "phpspec/prophecy": "^1.8",
        "phpspec/prophecy-phpunit": "^2",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./web"
            },
            "symlink": true,
            "overwrite": true,
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/web.config": false
            }
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "Ensure the table alias is no longer than 60 characters long": "https://www.drupal.org/files/issues/2022-06-30/571548-165.patch",
                "Form API #ajax not working on link element": "https://www.drupal.org/files/issues/2023-01-12/2915954-29.patch",
                "Add events for matching entity hooks": "https://www.drupal.org/files/issues/2023-03-20/2551893-109.patch",
                "PostgreSQL: email case insensitivity": "https://www.drupal.org/files/issues/2022-12-19/drupal-case_sensitive_email-2490294-58-d95.patch"
            },
            "drupal/flysystem": {
                "Add Drush commands for sending and receiving files": "https://www.drupal.org/files/issues/2023-08-31/3192777-4-added_drush_commands.patch",
                "Support new lazy asset collection optimizers": "https://git.drupalcode.org/project/flysystem/-/merge_requests/26.diff"
            },
            "drupal/govuk_notify": {
                "Message body formatting array to string": "https://www.drupal.org/files/issues/2021-08-11/3227617-2-govuk-notify-message-body-formatting.diff"
            },
            "drupal/govuk_theme": {
                "Remove style sheet": "https://www.drupal.org/files/issues/2023-08-30/3202722-6-remove_generated_css.patch",
                "Correct version dependencies": "./patches/yargs-version.patch"
            },
            "drupal/redis": {
                "Unable to override default scheme with settings": "https://www.drupal.org/files/issues/2021-02-16/redis-support_tls_on_predis-3004561-33.patch",
                "Take into account CONFIG command may not work": "https://www.drupal.org/files/issues/2021-02-16/redis-allow_disabled_config-3173988-4.patch"
            },
            "drupal/role_delegation": {
                "Temporary patch to enable module uninstall": "./patches/tmp-patch-fix-for-d9-upgrade.patch"
            },
            "drupal/search_api": {
                "Add support for rendering computed fields to the \"field\" views field handler": "https://git.drupalcode.org/project/search_api/-/merge_requests/75.diff"
            },
            "drupal/vbo_export": {
                "Allow output file scheme to be chosen": "https://www.drupal.org/files/issues/2021-11-23/allow_configurable_scheme-2988257-18.patch"
            },
            "drupal/views_data_export": {
                "Allow output file scheme to be chosen": "https://www.drupal.org/files/issues/2021-03-01/allow-configurable-filesystems_3200974-6.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "alias drush='/var/www/par/vendor/drush/drush/drush'"
        ]
    }
}
